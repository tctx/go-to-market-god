{
    "name": "template",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
      "pg": "^8.11.3",
      "@browserbasehq/stagehand": "^3.0.7",
      "playwright": "^1.57.0",
      "zod": "^3.23.8",
      "openai": "^4.73.0"
    },
    "devDependencies": {
      "npm-run-all": "^4.1.5",
      "concurrently": "^8.2.2"
    },
    "scripts": {
      "setup-project": "npm install && npx run-s python:verify project:port mcp:pick-auth-token mcp:pick-router-port mcp:install mcp:register mcp:router project:tracker",
      "setup-step2": "npx run-s python:verify project:port mcp:pick-auth-token mcp:pick-router-port mcp:install mcp:register mcp:router project:tracker",
      "python:verify": "node utilities/project-setup/js-scripts/verify-python.js",
      "github:init": "./utilities/project-setup/init-github-repo.sh",
      "project:port": "node utilities/project-setup/js-scripts/pick-port.js",
      "project:tracker": "node utilities/project-setup/js-scripts/generate-project-tracker.js",
      "mcp:pick-auth-token": "node utilities/project-setup/js-scripts/pick-auth-token.js",
      "mcp:pick-router-port": "node utilities/project-setup/js-scripts/pick-mcp-router-port.js",
      "mcp:install":  "npm --prefix mcp/mcp-logs install",
      "mcp:register": "npm --prefix mcp/mcp-logs run mcp:register",
      "mcp:router":   "npx concurrently -k -n router \"npm --prefix mcp/mcp-logs run mcp:router\"",
      "hubspot:fetch-api": "cd product && node fetch-hubspot-api-defs.js > ../hubspot-api-defs.json",
      "hubspot:prepare": "cd product && node prepare-atlas-prompt.js",
      "hubspot:runner": "node product/script-runner.js",
      "gtm:gui": "node gui/server.js"
    }
  }
  
